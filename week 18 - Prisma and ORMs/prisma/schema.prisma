generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  email    String @unique
  password String
}

model Todo {
  id          Int     @id @default(autoincrement())
  title       String
  description String
  isDone        Boolean
  userId      Int
}
